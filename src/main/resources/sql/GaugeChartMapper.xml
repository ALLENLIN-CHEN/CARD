<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.scut.mychart.mapper.GaugeChartMapper">
	<!-- 开启二级缓存 -->
	<cache size="1024" readOnly="true" />
	
	<select id="getChartTotalByYear" resultType="org.scut.mychart.model.GaugeChartModel" useCache="true">
		<![CDATA[
			select year(aae036) as year,count(*) as total from jc14 group by year(aae036) order by year
		]]>
	</select>
</mapper>